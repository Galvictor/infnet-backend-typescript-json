{
    "name": "infnet-backend-typescript",
    "version": "1.0.0",
    "description": "Backend básico em TypeScript",
    "main": "dist/main.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/main.js",
        "dev": "ts-node src/main.ts",
        "dev:watch": "nodemon --exec ts-node src/main.ts",
        "watch": "tsc --watch",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "test": "jest"
    },
    "keywords": [
        "typescript",
        "backend",
        "express"
    ],
    "author": "João Victor",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "express-validator": "^7.2.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@eslint/json": "^0.13.1",
        "@eslint/markdown": "^7.1.0",
        "@types/express": "^4.17.21",
        "@types/express-validator": "^3.0.2",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.2.0",
        "@typescript-eslint/utils": "^8.39.1",
        "eslint": "^9.33.0",
        "globals": "^16.3.0",
        "jest": "^30.0.5",
        "jiti": "^2.5.1",
        "nodemon": "^3.0.2",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0"
    }
}
